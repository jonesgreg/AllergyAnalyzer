<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
  tools:context=".HomeActivity">

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginTop="-61dp"
        android:background="@color/AmberBackground"
        android:visibility="visible"

        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentStart="true"
            android:orientation="horizontal"
            tools:ignore="RtlHardcoded,UselessParent">

        </LinearLayout>

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView8"
            android:layout_marginEnd="31dp"
            android:layout_marginTop="77dp"
            android:layout_toStartOf="@+id/imageButton9"
            android:layout_weight="1"
            android:contentDescription=""
            app:srcCompat="@drawable/home"
            tools:ignore="ContentDescription,ObsoleteLayoutParam" />

        <ImageButton
            android:id="@+id/imageButton9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/imageButton4"
            android:layout_centerHorizontal="true"
            android:layout_weight="1"
            android:contentDescription=""
            app:srcCompat="@drawable/profile"
            tools:ignore="ContentDescription,ObsoleteLayoutParam" />

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/imageButton9"
            android:layout_marginStart="23dp"
            android:layout_toEndOf="@+id/imageButton9"
            android:layout_weight="1"
            android:contentDescription=""
            app:srcCompat="@drawable/search"
            tools:ignore="ContentDescription,ObsoleteLayoutParam" />

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:contentDescription=""
            app:srcCompat="@drawable/groups"
            tools:ignore="ContentDescription,ObsoleteLayoutParam"
            android:layout_alignTop="@+id/imageButton7"
            android:layout_toEndOf="@+id/imageButton4" />

        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:contentDescription=""
            app:srcCompat="@drawable/help"
            tools:ignore="ContentDescription,ObsoleteLayoutParam"
            android:layout_below="@+id/imageButton4"
            android:layout_alignStart="@+id/imageButton4"
            android:layout_marginTop="15dp" />

        <ImageButton
            android:id="@+id/imageButton8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/imageButton5"
            android:layout_alignTop="@+id/imageButton6"
            android:layout_weight="1"
            android:contentDescription=""
            app:srcCompat="@drawable/about"
            tools:ignore="ContentDescription,ObsoleteLayoutParam" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/imageButton5"
            android:layout_alignParentTop="true"
            android:layout_alignStart="@+id/imageButton4"
            android:layout_marginTop="72dp"
            android:adjustViewBounds="true"
            android:cropToPadding="false"
            android:scaleType="fitEnd"
            android:visibility="visible"
            app:srcCompat="@drawable/allergy" />

        <ImageButton
            android:id="@+id/imageButton11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/googlemap"
            tools:ignore="ContentDescription"
            android:layout_below="@+id/imageButton7"
            android:layout_alignStart="@+id/imageButton7"
            android:layout_marginTop="22dp" />

        <ImageButton
            android:id="@+id/imageButton12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/login"
            tools:ignore="ContentDescription"
            android:layout_alignTop="@+id/imageButton11"
            android:layout_alignStart="@+id/imageButton8" />

    </RelativeLayout>


</android.support.constraint.ConstraintLayout>
